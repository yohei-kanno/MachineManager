tbody id="place-#{place.id}"
  td = place.name
  td = "#{count_of_pachinko(place)}台"
  td = "#{count_of_slot(place)}台"
  td = "#{count_of_all(place)}台"
  td = place.created_at.to_s(:datetime_jp)
  td = render "edit",{place: place}
  td = link_to "削除", store_place_path(@store, place), method: :delete, data:{confirm: "本当に削除しますか？"}, class: "link-danger"
